    <rt-prop name='ThemeSamples'>
        <ComponentSample>
            <rt-prop name='Visuals'>
                <LC.Buttons rt-fs='true'>
                    <LC.Button
                     class='caution'
                     Label='"Submit"'
                     Level='~Cautionary'
                     State='^LowLevel (~Actionable Actions.greet)'/>
                </LC.Buttons>
            </rt-prop>
            <rt-prop name='Code |> ~Children'>
                <Code Language='~Render'><![CDATA[
                    <LC.Button
                     class='caution'
                     Label='"Submit"'
                     Level='~Cautionary'
                     State='^LowLevel (~Actionable Actions.greet)'/>
                ]]></Code>
                <Code Language='~Fsharp' Heading='"Styles"'><![CDATA[
                    "caution" ==>
                    LibClient.Components.ButtonStyles.Theme.One (
                        level              = LibClient.Components.Button.Cautionary,
                        state              = LibClient.Components.Button.State.Actionable ignore,
                        theTextColor       = Color.Black,
                        theBorderColor     = Color.White,
                        theBackgroundColor = Color.DevOrange
                    )
                ]]></Code>
            </rt-prop>
        </ComponentSample>

        <ComponentSample>
            <rt-prop name='Visuals'>
