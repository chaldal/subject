                <rt-prop name='Visuals'>
                    <Map.Base
                        class='map'
                        ApiKey='AppEggShellGallery.Config.current().GoogleMapsApiKey'/>
                </rt-prop>
                <rt-prop name='Code |> ~singleBlock ~Render'><![CDATA[
                    <Map.Base
                        class='map'
                        ApiKey='AppEggShellGallery.Config.current().GoogleMapsApiKey'/>
                ]]></rt-prop>
            </ComponentSample>

            <ComponentSample>
                <rt-prop name='Visuals'>
