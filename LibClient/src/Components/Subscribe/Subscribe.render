<rt-root rt-type-parameters="'T">
    <rt-match what='props.With'>
        <rt-case is='With.WhenAvailable render'>
            <LC.AsyncData rt-fs='true'
             Data='estate.Value'
             WhenAvailable='render'/>
        </rt-case>
        <rt-case is='With.Raw render'>
            {=render estate.Value}
        </rt-case>
    </rt-match>
</rt-root>
