     javaScriptEnabled='true'
     ref='actions.Bound.RefWebview'
     source='{ html = actions.source (props.SiteKey); baseUrl = Some props.BaseUrl }'
     onMessage='actions.onMessageHandler' />
     {=props.With (actions.getToken)}
</rt-block>